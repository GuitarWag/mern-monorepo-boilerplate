{
  "name": "api",
  "version": "1.0.4",
  "private": true,
  "scripts": {
    "start": "yarn prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./build/bin/www",
    "dev": "NODE_ENV=development yarn server",
    "clean": "rimraf build",
    "build": "babel ./src --out-dir build",
    "prod": "NODE_ENV=production yarn clean && yarn build && yarn server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "@workspace/common": "^1.0.4",
    "@workspace/constants": "^1.0.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2"
  }
}
