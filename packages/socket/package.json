{
  "name": "@workspace/socket",
  "version": "1.0.6",
  "private": true,
  "scripts": {
    "start": "yarn prod",
    "server": "babel-node ./src/index.js",
    "server:prod": "node ./build/index.js",
    "dev": "NODE_ENV=development yarn server",
    "clean": "rimraf build",
    "build": "babel ./src --out-dir build",
    "prod": "NODE_ENV=production yarn clean && yarn build && yarn server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "@workspace/constants": "^1.0.6",
    "dotenv": "^8.1.0",
    "expo-yarn-workspaces": "^1.2.0",
    "express": "~4.16.1",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2"
  }
}
