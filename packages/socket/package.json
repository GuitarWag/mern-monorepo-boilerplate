{
  "name": "socket",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "start": "yarn prod",
    "server": "babel-node ./src/index.js",
    "server:prod": "node ./build/index.js",
    "dev": "NODE_ENV=development yarn server",
    "clean": "rimraf build",
    "build": "babel ./src --out-dir build",
    "prod": "NODE_ENV=production yarn clean && yarn build && yarn server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2"
  }
}
